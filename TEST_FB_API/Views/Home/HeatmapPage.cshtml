                minOpacity: 0.05,
                blur: .75
            };

            // create heatmap with configuration
            var heatmapInstance = h337.create(config);

            $('.heatmap-canvas').each(function () {
                $(this).css('z-index', 100);
            });

            // prepare chart for schema counter
            function initializeChartJS(label, dataset) {
                var $canvas = $('#schemaCountCanvas');
                console.log(dataset);
                chart = new Chart($canvas, {
                    type: 'horizontalBar',
                    data: {
                        labels: label,
                        datasets: dataset
                    },
                    options: {
                        responsive: false,
                        title: {
                            display: true,
                            text: "Movie Click Count"
                        },
                        tooltips: {
                            enabled: false
                        },
                        scales: {
                            legend: {
                                position: 'top',
                            },
                            xAxes: [{
                                scaleLabel: {
                                    display: true,
                                    labelString: 'Count'
                                },
                                ticks: {
                                    beginAtZero: true
                                }
                            }],
                            yAxes: [{
                                scaleLabel: {
                                    display: true,
                                    labelString: 'Movie'
                                },
                            }]
                        }
                    }
                });
            }

            function retrieveData() {
                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("RetrieveData", "Home")',
                    success: function (response) {
                        console.log(JSON.parse(response.DataPointListJSON));
                        console.log(response.SchemaListCount);
                        console.log(response.SchemaList);
                        if (response.IsSuccess) {
                            var data = {
                                max: 3,
                                min: 0,
                                data: JSON.parse(response.DataPointListJSON)
                            };
                            heatmapInstance.setData(data);
                            
                            var schemaCount = response.SchemaListCount;
                            chart.data.datasets.forEach((dataset) => {
                                dataset.data = schemaCount;
                            });
                            chart.update();
                            var $clickLog = $('#clickLog')
                            $(response.SchemaList).each(function () {
                                var jsonLD = JSON.parse(this);
                                var movieName = jsonLD.name;
                                var datePublished = jsonLD.datePublished;
                                var directorList = "";
                                $(jsonLD.director).each(function () {
                                    directorList += this.name + ',<br />';
                                });
                                directorList = directorList.slice(0, directorList.length - 7);

                                var actorList = "";
                                $(jsonLD.actor).each(function () {
                                    actorList += this.name + ',<br />';
                                });
                                actorList = actorList.slice(0, actorList.length - 7);
                                
                                var bgColor = clickLogIndex % 2 == 0 ? 'rgba(0, 0, 0, 0.3)' : '#FFF';
                                $('<div>').append($('<p>')
                                            .css('background-color', bgColor)
                                            .append($('<span>')
                                                .css('width', '100%')
                                                .css('display', 'inline-block')
                                                .css('border', '1px solid black')
                                                .html('No. ' + clickLogIndex)
                                            )
                                            .append($('<br />'))
                                            .append($('<span>')
                                                .css('width', '100%')
                                                .css('display', 'inline-block')
                                                .css('border', '1px solid black')
                                                .html('Title :<br />' + movieName)
                                            )
                                            .append($('<br />'))
                                            .append($('<span>')
                                                .css('width', '100%')
                                                .css('display', 'inline-block')
                                                .css('border', '1px solid black')
                                                .html('Date Published :<br />' + datePublished)
                                            )
                                            .append($('<br />'))
                                            .append($('<span>')
                                                .css('width', '100%')
                                                .css('display', 'inline-block')
                                                .css('border', '1px solid black')
                                                .html('Director(s) :<br />' + directorList + '<br />')
                                            )
                                            .append($('<br />'))
                                            .append($('<span>')
                                                .css('width', '100%')
                                                .css('display', 'inline-block')
                                                .css('border', '1px solid black')
                                                .html('Actor(s) :<br />' + actorList)
                                            )
                                        ).prependTo($clickLog)
                                clickLogIndex++;
                            });
                        }
                        else {
                            alert(response.ErrorMsg);
                        }
                    },
                    complete: function (response) {
                        setTimeout(retrieveData, interval);
                    }
                });
            }
            
            $.ajax({
                type: 'POST',
                url: '@Url.Action("RetrieveSchemaListLabel", "Home")',
                success: function (response) {
                    var label = response;
                    if (label.length > 0) {
                        var data = [];
                        var datasets = [];
                        $(label).each(function () {
                            data.push(0);
                        });
                        
                        datasets.push({
                            label: "Count",
                            data: data,
                            backgroundColor: 'rgba(54, 162, 235, 0.2)',
                            borderColor: 'rgba(54, 162, 235, 1)',
                            borderWidth: 1
                        })
                        
                        initializeChartJS(label, datasets);
                        setTimeout(retrieveData, interval);
                    }
                }
            });
        });
    </script>

